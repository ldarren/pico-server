{
    "app":{
        "name": "WebChat",
        "env": "dev",
        "elements": ["actions", "models"]
    },
    "lib":{
        "webServer":{
            "mod":"web",
            "port":4888,
            "updateRate":60000
        },
        "redisChannel":{
            "mod":"redis",
            "host":"localhost",
            "port":6379,
            "db":10,
            "options":{}
        },
        "ritbAnalytics":{
            "mod":"analytics",
            "trackingId":"UA-44242154-4",
            "defaultClientId":"nodejs",
            "secure":false,
            "options":{
                "ni":1
            }
        }
    }
}
